public class Greatest {
    public static Double max(Double num1, Double num2, Double num3){
        if(num1 > num2){
            if(num1 > num3){
                return num1;
            }else{
                return num3;
            }
        }else{
            if(num2 > num3){
                return num2;
            }else{
                return num3;
            }
        }
    }
}